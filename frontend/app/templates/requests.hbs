<div class="container">
  <div class="section">

    <!--   Icon Section   -->
    <div class="row">
      <div class="col s12 m12">
        <table>
          <thead>
            <tr>
                <th data-field="user">User</th>
                <th data-field="driver">Driver</th>
                <th data-field="state">State</th>
            </tr>
          </thead>

          <tbody>
            {{#each model}}
              <tr>
                <td>{{user.name}}</td>
                <td>{{driver.name}}</td>
                <td>{{state}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>

  </div>
  <br><br>

  <div class="section">

  </div>
</div>
